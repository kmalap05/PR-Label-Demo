{
  "labels": {
    "Feature": {
      "name": "feature",
      "colour": "00ff00",
      "description": "New feature."
    },
    "Bug Fix": {
      "name": "bug fix",
      "color": "00ff00",
      "description": "Fixes bugs."
    },
    "Documentation": {
      "name": "documentation",
      "color": "00ff00",
      "description": "Documentation."
    },
    "Other": {
      "name": "other",
      "color": "00ff00",
      "description": "Other changes."
    },
    "JavaScript": {
      "name": "javascript",
      "color": "008672",
      "description": "JavaScript changes."
    },
    "CSS": {
      "name": "css",
      "color": "008672",
      "description": "CSS changes."
    }
  },
  "pr": {
    "Feature": {
      "requires": 1,
      "conditions": [
        {
          "type": "descriptionMatches",
          "pattern": ".*\\[x\\] Feature.*"
        }
      ]
    },
    "Bug Fix": {
      "requires": 1,
      "conditions": [
        {
          "type": "descriptionMatches",
          "pattern": ".*\\[x\\] Bug Fix.*"
        }
      ]
    },
    "Documentation": {
      "requires": 1,
      "conditions": [
        {
          "type": "descriptionMatches",
          "pattern": ".*\\[x\\] Documentation.*"
        }
      ]
    },
    "Other": {
      "requires": 1,
      "conditions": [
        {
          "type": "descriptionMatches",
          "pattern": ".*\\[x\\] Other.*"
        }
      ]
    },
    "JavaScript": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "*.js"
        }
      ]
    },
    "CSS": {
      "requires": 1,
      "conditions": [
        {
          "type": "filesMatch",
          "glob": "*.css"
        }
      ]
    }
  }
}
